import matplotlib.pyplot as plt
import numpy as np


v = np.array([25,50,100,150,200,300,400,500,600,700,800,900,1000])
pr = np.array([])
np.insert(pr, 1, len(pr))
for i in range(len(v)):
    v[i] * 1000
       

plt.bar(pr,v)
plt.xlabel("Products")
plt.ylabel("Sales")
plt.title("Sales Data")
plt.show()

